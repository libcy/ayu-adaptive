  "description": "A modification of Ayu theme with Atom One, Adapta GTK and macOS Mojave color palette.",
  "version": "0.1.4",
  "publisher": "ccy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/libcy/ayu-adaptive"
  },
  "bugs": {
    "url": "https://github.com/libcy/ayu-adaptive/issues",
    "email": "teabyii@gmail.com"
  },
